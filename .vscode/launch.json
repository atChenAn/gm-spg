{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "DegbuMock",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "runtimeExecutable": "npm", //默认是node,这里改成npm
      "runtimeArgs": [
        "run-script", //别名 run
        "debug" //对应上npm scripts上的debug
      ],
      "port": 9771, //调试端口
      "stopOnEntry": true
    },

    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\dist\\spg.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
